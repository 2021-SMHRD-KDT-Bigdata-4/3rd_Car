<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.car.mapper.CMapper">
    
    <select id="loginFunction" parameterType="memberVO" resultType="memberVO">
	 SELECT*from member where memberid=#{memberid} and password=#{password}
	</select>

    
    <delete id="boardDelete" parameterType="int">
        delete from tbl_board where idx=#{idx}
    </delete>
    
    
</mapper>
